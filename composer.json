{
    "name": "goldfinch/google-maps",
    "description": "The module helps to create and manage Google Maps in your Silverstripe project",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "google-maps",
        "silverstripe-google-maps",
        "map-events",
        "marker-events",
        "advancedmarkerelement-events",
        "info-window-events"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Art Karasev",
            "email": "art.karasev@proton.me"
        }
    ],
    "require": {
        "php": ">=8.0",
        "silverstripe/framework": "^5.0",
        "silverstripe/admin": "^2.0",
        "goldfinch/google-fields": "^1.0",
        "goldfinch/json-editor": "^1.0",
        "goldfinch/helpers": "^1.0",
        "goldfinch/taz": "^1.0",
        "jonom/silverstripe-someconfig": "^1.0",
        "silvershop/silverstripe-hasonefield": "dev-main"
    },
    "extra": {
        "expose": ["client/dist"]
    },
    "autoload": {
        "psr-4": {
            "Goldfinch\\GoogleMaps\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
